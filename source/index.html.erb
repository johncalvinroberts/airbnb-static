  <body class="text-center">
      <!-- Your banner code -->
    <div class="banner text-center" style="background: linear-gradient(100deg, rgba(0,101,168,0.6) 0%, rgba(0,36,61,0.6) 50%), url('images/background.jpg');">
      <div class="banner-content">
        <h1>WELCOME HOME</h1>
        <p>Rent unique places to stay from local hosts in 190+ countries.</p>
        <a href="" class="btn btn-default" role="button">How It Works</a>
      </div>
    </div>
    <!-- Your features code -->
    <div class="wrapper-grey padded text-center">
      <div class="container">
        <div class="row">
          <div class="col-xs-12 col-sm-6 col-md-3">
            <%= image_tag "globe.png", class:"feature-icon" %>
            <h2>Discover the world</h2><p>The earth is your new home.</p>
          </div>
          <div class="col-xs-12 col-sm-6 col-md-3">
            <%= image_tag "dollar.png", class:"feature-icon" %>
            <h2>Pay online</h2><p>No cash issues anymore.</p>
          </div>
          <div class="col-xs-12 col-sm-6 col-md-3">
          <%= image_tag "beer.png", class:"feature-icon" %>
            <h2>Meet People</h2><p>Discover new culture and lifestyle.</p>
          </div>
          <div class="col-xs-12 col-sm-6 col-md-3">
            <%= image_tag "heart.png", class:"feature-icon" %>
            <h2>Share and love</h2><p>Join a community you will love.</p>
          </div>
        </div>
      </div>
    </div>
    <div class="wrapper-dark-grey padded text-center">
      <div class="container">
        <div class="row">
          <% data.flats.each do |owner, flat| %>
            <div class="col-xs-12 col-sm-4">
              <div class="card" style="background: linear-gradient(rgba(24,42,69,0.1), rgba(215,222,234,0.3), rgba(24,42,69,0.5)), url('<%= image_path flat.flat_picture %>');">
                <a href="#" class="card-overlay"></a>
                <div class="card-category">
                  <%= flat.city %>
                </div>
                <div class="card-description">
                  <h2><%= flat.title %></h2>
                  <p class="card-intro" ><%= flat.description %></p>
                </div>
                <%= image_tag flat.owner_picture, class:"avatar-card" %>
              </div>
            </div>
            <% end %>
          </div>
      </div>
    </div>


    <!-- Your newsletter code -->
    <div class="wrapper-grey padded text-center">
      <div class="container">
        <div class="row">
          <div class="col-xs-12">
              <h1>Get news from us</h1>
              <p>Leave us your mail and get news from us</p>
              <form class="form-inline">
              <div class="form-group">
                <input type="email" class="form-control" id="exampleInputEmail2" placeholder="dave.fuck@gmail.com">
                <button type="submit" class="btn btn-default">Subscribe</button>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>

<div id="map" style="width: 100%; height: 400px;"></div>

<script type="text/javascript" src="https://maps.googleapis.com/maps/api/js"></script>

<script type="text/javascript">
var myLatlng = new google.maps.LatLng(48.8655909, 2.378983);

var myOptions = {
  zoom: 16,
  center: myLatlng,
  scrollwheel: false,
  mapTypeId: google.maps.MapTypeId.ROADMAP,
  styles: [] // TODO: replace [] by array from https://snazzymaps.com/
};

var map = new google.maps.Map(document.getElementById('map'), myOptions);
var marker = new google.maps.Marker({
  position: myLatlng,
  map: map,
  title: "get busy on airbnb~~"
});
</script>

